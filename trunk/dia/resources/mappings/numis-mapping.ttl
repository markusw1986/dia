map:database a d2rq:Database;
	d2rq:jdbcDriver "com.mysql.jdbc.Driver";
	d2rq:jdbcDSN "jdbc:mysql://localhost:3306/numis";
	d2rq:username "root";
	d2rq:password "";
	jdbc:autoReconnect "true";
	jdbc:zeroDateTimeBehavior "convertToNull";
	.
##########
# coin  
##########
map:coin a d2rq:ClassMap;
	d2rq:dataStorage map:database;
	d2rq:uriPattern "coin/@@numis.id@@";
	d2rq:class nm:coin;
	.
####################
# start and enddate
####################
map:coin_begin_periode a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:coin;
	d2rq:property nm:start_date;
	d2rq:propertyDefinitionLabel "start date";
	d2rq:column "numis.begin_periode";
	d2rq:datatype xsd:string;
	.
map:coin_eind_periode a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:coin;
	d2rq:property nm:end_date;
	d2rq:propertyDefinitionLabel "end date";
	d2rq:column "numis.eind_periode";
	d2rq:datatype xsd:string;
	.	
#############
# findtype
#############
map:coin_karakter_vondst a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:coin;
	d2rq:property coin:hasFindtype;
	d2rq:propertyDefinitionLabel "findtype";
	d2rq:uriColumn "numis.karakter_vondst";
	d2rq:translateWith map:FindtypeTable;
	.
	
map:FindtypeTable a d2rq:TranslationTable;
	d2rq:javaClass "de.unifrankfurt.informatik.dbis.d2rqextension.PatternTranslator";
	patt:translatePattern [
		patt:type patt:Before; 
		patt:delimiter "(";
		patt:delimiter "/";
	];
	patt:translation [ patt:databaseValue "losse vondst"; patt:rdfValue coin:Single; ];
	patt:translation [ patt:databaseValue "schatvondst"; patt:rdfValue coin:Hoard; ];
	patt:translation [ patt:databaseValue "schatvondst tpq 793"; patt:rdfValue coin:Hoard; ];
	.
	
##########
# period
##########
map:coin_staat a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:coin;
	d2rq:property coin:hasPeriod;
	d2rq:propertyDefinitionLabel "period";
	d2rq:uriColumn "numis.staat";
	d2rq:translateWith map:PeriodTable;
	.
	

