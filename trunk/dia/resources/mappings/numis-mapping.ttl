map:coin_issuer_unknown a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:coin;
	d2rq:property coin:hasAuthority;
	d2rq:condition "numis.autoriteit like '(%' OR numis.autoriteit like 'late%' OR numis.autoriteit like '4e%' OR numis.autoriteit like '5e%' OR numis.autoriteit like '6e%' OR numis.autoriteit like 'anoniem%' OR numis.autoriteit like 'barbaars%' OR numis.autoriteit like 'barbaaars%' OR numis.autoriteit like 'Arabisch%' OR numis.autoriteit like 'Auguste%'";
	d2rq:constantValue coin:Unknown;
.


map:coin_has_authority a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:coin;
	d2rq:refersToClassMap map:authority;
	d2rq:property coin:hasAuthority;
	d2rq:propertyDefinitionLabel "hasAuthority";
.


map:authority a d2rq:ClassMap;
	d2rq:dataStorage map:database;
	d2rq:class nm:authority;
	d2rq:propertyDefinitionLabel "authority";
	d2rq:uriColumn "numis.autoriteit";
	d2rq:translateWith map:AuthorityTable;
.

