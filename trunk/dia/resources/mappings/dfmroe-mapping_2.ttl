map:CoinedForTable a d2rq:TranslationTable;
	d2rq:javaClass "de.unifrankfurt.informatik.dbis.d2rqextension.PatternTranslator";
	patt:translatePattern [
		patt:type patt:After; 
		patt:delimiter "f?r";
	]
	
